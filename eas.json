{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "buildConfiguration": "Release"
      },
      "cache": {
        "disabled": true
      },
      "env": {
        "EXPO_NO_DOTENV": "1",
        "EXPO_IMAGE_CACHE_DIRECTORY": "/tmp/expo-image-cache"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
