{
  "expo": {
    "name": "TetraTalk",
    "slug": "tetratalk",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.tetratalk.app",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-5837885590326347~5503619440"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tetratalk.app",
      "versionCode": 3,
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-5837885590326347~2090169804"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "jihunjo",
    "plugins": [
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-5837885590326347~2090169804",
          "iosAppId": "ca-app-pub-5837885590326347~5503619440",
          "delayAppMeasurementInit": false
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c029ee88-7475-4a19-95d6-0c161a935451"
      }
    }
  }
}
